{
    "name": "jermashley/laravel-uno-theme",
    "description": "Site theme for Flat World family of sites.",
    "type": "laravel",
    "license": "MIT",
    "authors": [
        {
            "name": "Jeremiah Ashley",
            "email": "jeremiah@prologuetechnology.com"
        }
    ],
    "require": {
        "illuminate/support": "^5.4",
        "php" : "~7.0"
    },
    "autoload": {
        "psr-4": {
            "Jermashely\\LaravelUnoTheme\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Jermashely\\LaravelUnoTheme\\UnoServiceProvider"
            ]
        }
    },
    "config": {
        "minimum-stability": "dev",
        "prefer-stable": true
    }
}
